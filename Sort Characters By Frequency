class Solution:
    def frequencySort(self, s: str) -> str:
        freq = {}
        for ch in s:
            freq[ch] = freq.get(ch, 0) + 1

        
        chars = sorted(freq.items(), key=lambda x: -x[1])

        res = []
        for ch, count in chars:
            res.append(ch * count)

        return "".join(res)
